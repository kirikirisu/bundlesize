{
  "compilerOptions": {
    "target": "es2019",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "declaration": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "isolatedModules": true,
    "outDir": "./dist"
  }
}
